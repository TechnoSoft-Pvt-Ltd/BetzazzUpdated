
@{
    ViewBag.Title = "PreMatch";
    Layout = "~/Views/_Betzazz.cshtml";
}

<div class="content-body content-margins">
    <!-- Revenue, Hit Rate & Deals -->
    <div class="row">
        <div class="col-lg-12 " style="padding-left:0px;padding-right:0px;">

            <div class="card inplay" >
                @*This card for Cricket Inplay Events*@
                <h4 class="flex-row--title text-center">Comming Soon</h4>
                <div class="card-body my-card-body" >

                    <div class="col col--xs-8 col--sm-10 col--md-10 col--lg-11">
                        <span class="flex-row--title text-left"><h4 >Cricket</h4></span>

                        <div class="col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv" >
                            @*<span class="caret"></span> <span></span>*@
                            <ul class="list-unstyled components ">
                                <li class="active">
                                    <a href="#Testinplayview" data-toggle="collapse" aria-expanded="false">
                                        <div class="dropdown-center-settings"> @ViewBag.PMLeagueName  </div>
                                    </a>
                                    <ul class="collapse list-unstyled" id="Testinplayview">
                                        <li>
                                            @for (int i = 0; i < ViewBag.ArrPMTestData.Length; i++)
                                             {
                                               
                                                <button type="button" id="@ViewBag.PMTestEvntid[i]" class="btn  button1 col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv"
                                                        value="btn_Onclick" onclick="location.href='@Url.Action("btn_Onclick", "PreMatch")?Eventid='+ @ViewBag.PMTestEvntid[i]" style="height:auto">
                                                    @ViewBag.ArrPMTestData[i]  <br />  @ViewBag.TestEventDate[i]
                                                </button>
                                             }
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <div class="col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv">
                            <ul class="list-unstyled components ">
                                <li class="active">
                                    <a href="#T20Div" data-toggle="collapse" aria-expanded="false">
                                        <div class="dropdown-center-settings"> @ViewBag.PMT20LeagueName  </div>
                                    </a>
                                    <ul class="collapse list-unstyled" id="T20Div">
                                        <li>

                                            @for (int i = 0; i < ViewBag.ArrPMT20Data.Length; i++)
                                            {

                                            <button type="button" id="@ViewBag.PMT20Evntid[i]" class="btn  button1 col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv"
                                                    value="btn_Onclick" onclick="location.href='@Url.Action("btn_Onclick", "PreMatch")?Eventid='+ @ViewBag.PMT20Evntid[i]" style="height:auto">
                                                @ViewBag.ArrPMT20Data[i]  <br />  @ViewBag.T20EventDate[i]
                                            </button>
                                            }
                                        </li>
                                        
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <div class="col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv">

                            <ul class="list-unstyled components ">
                                <li class="active">
                                    <a href="#ODiDiv" data-toggle="collapse" aria-expanded="false">
                                        <div class="dropdown-center-settings"> @ViewBag.PMODILeagueName  </div>
                                    </a>
                                    <ul class="collapse list-unstyled" id="ODiDiv">
                                        <li>

                                            @for (int i = 0; i < ViewBag.ArrPMODIData.Length; i++)
                                            {

                                            <button type="button" id="@ViewBag.PMODIEvntid[i]" class="btn  button1 col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv"
                                                    value="btn_Onclick" onclick="location.href='@Url.Action("btn_Onclick", "PreMatch")?Eventid='+ @ViewBag.PMODIEvntid[i]" style="height:auto">
                                                @ViewBag.ArrPMODIData[i]  <br />  @ViewBag.ODIEventDate[i]
                                            </button>
                                            }
                                        </li>
                                        
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <div class="col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv">

                            <ul class="list-unstyled components ">
                                <li class="active">
                                    <a href="#T20Intinplayview" data-toggle="collapse" aria-expanded="false">
                                        Twenty 20 Internationals
                                    </a>
                                    <ul class="collapse list-unstyled" id="T20Intinplayview">
                                        <li><a href="#">Home 1</a></li>
                                        <li><a href="#">Home 2</a></li>
                                        <li><a href="#">Home 3</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <div class="col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv">
                            <ul class="list-unstyled components ">
                                <li class="active">
                                    <a href="#PLinplayview" data-toggle="collapse" aria-expanded="false">
                                        Premier league
                                    </a>
                                    <ul class="collapse list-unstyled" id="PLinplayview">
                                        <li><a href="#">Home 1</a></li>
                                        <li><a href="#">Home 2</a></li>
                                        <li><a href="#">Home 3</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>

                    </div>
                </div>

