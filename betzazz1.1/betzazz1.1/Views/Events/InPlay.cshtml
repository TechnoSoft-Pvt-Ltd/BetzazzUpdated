
@using System.Linq;
@{
    ViewBag.Title = "InPlay";
    Layout = "~/Views/_Betzazz.cshtml";
}

<div class="content-body content-margins">
    <!-- Revenue, Hit Rate & Deals -->
    <div class="row">
        <div class="col-lg-12" style="padding-left:0px;padding-right:0px;">

            <div class="card inplay">
                @*This card for Cricket Inplay Events*@
                <h4 class="flex-row--title text-center">In-Play</h4>
                <div class="card-body my-card-body">

                    <div class="col col--xs-8 col--sm-10 col--md-10 col--lg-11">
                        <span class="flex-row--title text-left"><h4 >Cricket</h4></span>

                        <div class="col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv">
                            @*<span class="caret"></span> <span></span>*@
                            <ul class="list-unstyled comments ">
                                <li class="active">
                                    <a href="#Testinplayview" data-toggle="collapse" aria-expanded="false">
                                      <div class="dropdown-center-settings"> @ViewBag.LeagueName  </div>
                                    </a>
                                    <ul class="collapse list-unstyled" id="Testinplayview">
                                       
                                       
                                        @for(int i=0;i<ViewBag.ArrTestData.Length; i++)
                                         {

                                            <li>                                           
                                                <button type="button" id="@ViewBag.Evntid[i]" class="btn  button1 col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv">
                                                    @ViewBag.ArrTestData[i]
                                                </button>
                                            </li>
                                         }
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <div class="col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv">
                            <ul class="list-unstyled components ">
                                <li class="active">
                                    <a href="#Ondayinplayview" data-toggle="collapse" aria-expanded="false">
                                        <div class="dropdown-center-settings">  @ViewBag.T20LeagueName  </div>
                                    </a>
                                    <ul class="collapse list-unstyled" id="Ondayinplayview">
                                        @for (int j = 0; j < ViewBag.ArrT20Data.Length; j++)
                                        {
                                            <li>
                                                <button type="button" id="@ViewBag.T20EventId[j]" class="btn  button1 col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv">
                                                    @ViewBag.ArrT20Data[j]
                                                </button>
                                            </li>
                                        }
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <div class="col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv">

                            <ul class="list-unstyled components ">
                                <li class="active">
                                    <a href="#ODI1inplayview" data-toggle="collapse" aria-expanded="false">
                                    <div class="dropdown-center-settings">  @ViewBag.ODILeagueName  </div>
                                    </a>
                                    <ul class="collapse list-unstyled" id="ODI1inplayview">
                                        @for (int k = 0; k < ViewBag.ArrODIData.Length; k++)
                                         {
                                            <li>
                                                <button type="button" id="@ViewBag.ODIEventId[k]" class="btn  button1 col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv">
                                                    @ViewBag.ArrODIData[k]
                                                </button>
                                            </li>
                                         }
                                    </ul>
                                </li>
                            </ul>
                        </div>
