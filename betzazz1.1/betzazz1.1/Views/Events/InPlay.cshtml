                                        {
                                            <li>
                                                <button type="button" id="@ViewBag.ODIEventId[k]" class="btn  button1 col col--xs-8 col--sm-10 col--md-4 col--lg-4 text-left eventbDiv">
                                                    @ViewBag.ArrODIData[k]
                                                </button>
                                            </li>
                                        }
